<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home">

    <android.widget.Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnyoga"
        android:text="요가"
        android:textSize="50dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="80dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"></android.widget.Button>
    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></fragment>

    <android.widget.Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnpila"
        android:text="필라테스"
        app:layout_constraintTop_toTopOf="@id/btnyoga"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="100dp"
        android:textSize="50dp"></android.widget.Button>

    <android.widget.Button
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:id="@+id/btnhealth"
        android:text="헬스"
        app:layout_constraintTop_toTopOf="@id/btnpila"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="100dp"
        android:textSize="50dp"></android.widget.Button>

    <include
        android:id="@+id/buttonlayout"
        layout="@layout/buttonlayout"></include>
</androidx.constraintlayout.widget.ConstraintLayout>